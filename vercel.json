{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["sfo1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NODE_OPTIONS": "--no-warnings",
      "VERCEL_BUILD_STEP": "true"
    }
  },
  "env": {
    "NODE_OPTIONS": "--no-warnings",
    "VERCEL_BUILD_STEP": "true"
  },
  "routes": [
    {
      "src": "/api/migrate-images(.*)",
      "dest": "/api/migrate-images"
    },
    {
      "src": "/api/repair-images(.*)",
      "dest": "/api/repair-images"
    }
  ],
  "github": {
    "silent": true
  }
}
